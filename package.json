{
  "name": "browser-db",
  "version": "1.0.0",
  "description": "Type-safe IndexedDB wrapper with chainable transactions",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "indexeddb",
    "idb",
    "database",
    "storage",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "fake-indexeddb": "^6.2.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
